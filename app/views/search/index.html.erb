<h2>Search our network for a podcast to add to Podsmack</h2>

<%= form_tag(search_path, method: "get") do %>
  <div class="container">
    <div class="row">
      <%= label_tag "Podcast Name" %>
      <%= text_field_tag :podcast_name %> 
      <%= submit_tag 'Search' %>
    </div>
  </div>
<% end %>
