<h1> <%= @podcast.name %> </h1>
<%= image_tag(@podcast.photo) %>
<p> Description: <%= @podcast.description %> </p>
<p> Location: <%= @podcast.location %> </p>
<p> twitter: <%= @podcast.twitter %> </p>
<p> patreon: <%= @podcast.patreon %> </p>
<p> instagram: <%= @podcast.instagram %> </p>
<p> facebook: <%= @podcast.facebook %> </p>
<p> facebook: <%= @podcast.facebook %> </p>
<%= button_to 'Follow', followings_path(podcast_id: @podcast.id, user_id: current_user.id), method: :post %>

<section class="tags">
  <% @podcast.tags.each do |tag| %>
    tag.name
  <% end %>
</section>
